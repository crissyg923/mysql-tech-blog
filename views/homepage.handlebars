<!-- Loop over each object in the projects array -->
<h1>Current Blog Posts</h1>
<div class="row">
{{#each posts as |post| }}
<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title"> {{ post.title }} </h5>
    
    <p class="card-text"> {{ post.body }} </p>
    <a href="#" class="card-link">View More</a>
  </div>
</div>
{{/each}}
</div>
